@model IEnumerable<MvcPractice.Models.HighScore>

@{
    ViewBag.Title = "Dragon Slayer V2 High Scores";
}

<h2>Dragon Slayer V2 Top 10</h2>
<ol>
    @foreach (var item in Model)
    {
        <li>
            @item.Name - @item.Score - @item.DateCreated.Value.ToShortDateString()
         </li>
    }
</ol>